# Superuser password used when provisioning the Postgres container.
POSTGRES_SUPERUSER_PASSWORD=change-me-superuser

# Application database credentials per environment
DEV_DB_PASSWORD=change-me-dev
STAGING_DB_PASSWORD=change-me-staging
PROD_DB_PASSWORD=change-me-prod

# Connection URLs for application usage
DEV_DATABASE_URL=postgres://gci_dev:change-me-dev@localhost:5432/gci_dev
STAGING_DATABASE_URL=postgres://gci_staging:change-me-staging@localhost:5432/gci_staging
PROD_DATABASE_URL=postgres://gci_prod:change-me-prod@localhost:5432/gci_prod

# JOFotara Credentials
USERNAME=jofotara_username
PASSWORD=jofotara_password
TAX_NUMBER=jofotara_tax_number